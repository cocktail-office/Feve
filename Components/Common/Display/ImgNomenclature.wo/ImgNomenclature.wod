CondOui: WOConditional {
	condition = isDansLaNomenclature;
}

CondNon: WOConditional {
	condition = isDansLaNomenclature;
	negate = true;
}

ImgOui: WOImage {
	filename = "images/database.png";
	width = "16px";
	height = "16px";
	border = "0";
	title = "dans la nomenclature (connu et codifié dans la base de données)";
}

ImgNon: WOImage {
	filename = "images/text.png";
	width = "16px";
	height = "16px";
	border = "0";
	title = "hors nomenclature (pas encore référencé)";
}